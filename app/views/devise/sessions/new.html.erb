<div class="row">
  <div class="col s12 m5">
    <div class="card blue-text text-darken-2">
      <div class="card-content black-text">
        <h5 style="color:grey;font-size: 20px;">Enter your credentials</h5>
        </br>
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="input-field">
            <%= f.email_field :email, autofocus: true, class: "validate"%>
            <label for="email">Email</label>
          </div>

          <div class="input-field">
            <%= f.password_field :password, autocomplete: "off", class: "validate" %>
            <label for="password">Password</label>
          </div>

          <% if devise_mapping.rememberable? -%>
            <div class="input-field">
              <%= f.check_box :remember_me, class: "check_box" %>
              <%= f.label :remember_me %>
            </div>
          <% end -%>
        </br>
          <div class="actions">
            <%= f.submit "Log in", class: "btn" %>
          </div>
        <% end %>
        </br>
        <%= render "devise/shared/links" %>
      </div>
    </div>    
  </div>
</div>